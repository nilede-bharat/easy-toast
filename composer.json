{
    "name": "nilede-bharat/easy-toast",
    "description": "Easy backend make toast notification for laravel+vue",
    "type": "library",
    "version": "1.0.0",
    "autoload": {
        "psr-4": {
            "Bharat\\EasyToast\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bharat\\EasyToast\\": "src/"
        }
    },
    "require": {
        "php": "^7.4 || ^8.0",
        "laravel/framework": "^8.0 || ^9.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/nilede-bharat/easy-toast"
        }
    ]
}
